{
  "name": "scm_web_app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "concurrently -r \"yarn start:server\" \"yarn start:client\"",
    "start:client": "react-scripts start",
    "start:server": "NODE_PATH=src/ nodemon server/index.js --exec babel-node",
    "db:seed": "yarn run db:drop && babel-node server/database/seed.js",
    "db:drop": "babel-node server/database/drop.js",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint",
    "prettier": "prettier --write \"**/*.{js,graphql,json,css}\""
  },
  "dependencies": {
    "@apollo/react-hoc": "^3.1.3",
    "@babel/cli": "^7.7.5",
    "@babel/core": "^7.7.5",
    "@babel/node": "^7.7.4",
    "@babel/preset-env": "^7.7.6",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "antd": "^3.26.3",
    "apollo-boost": "^0.4.4",
    "apollo-cache-inmemory": "^1.6.5",
    "apollo-client": "^2.6.8",
    "apollo-link": "^1.2.13",
    "apollo-link-context": "^1.0.19",
    "apollo-link-ws": "^1.0.19",
    "apollo-server": "^2.9.13",
    "apollo-server-core": "^2.9.16",
    "apollo-server-errors": "^2.3.4",
    "apollo-upload-client": "^12.1.0",
    "apollo-utilities": "^1.3.3",
    "aws-sdk": "^2.595.0",
    "bcryptjs": "^2.4.3",
    "concurrently": "^5.0.1",
    "cors": "^2.8.5",
    "debounce": "^1.2.0",
    "dotenv": "^8.2.0",
    "eslint-plugin-react-hooks": "^2.3.0",
    "express": "^4.17.1",
    "graphql": "^14.5.8",
    "graphql-import": "^0.7.1",
    "graphql-tag": "^2.10.2",
    "graphql-tools": "^4.0.6",
    "graphql-upload": "^10.0.0",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.24.0",
    "mongoose": "^5.8.1",
    "mongoose-transactions": "^1.1.4",
    "mongoose-unique-validator": "^2.0.3",
    "mysql2": "^2.0.2",
    "nodemon": "^2.0.1",
    "print-js": "^1.0.63",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-apollo": "^3.1.3",
    "react-cookies": "^0.1.1",
    "react-dom": "^16.12.0",
    "react-loadable": "^5.5.0",
    "react-map-styles": "^0.3.0",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.3.0",
    "react-scrollable-feed": "^1.1.2",
    "react-topbar-progress-indicator": "^4.0.2",
    "react-webcam": "^4.0.0",
    "recharts": "^2.0.0-beta.1",
    "sequelize": "^5.21.3",
    "shortid": "^2.2.15",
    "signale": "^1.4.0",
    "styled-components": "^4.4.1",
    "subscriptions-transport-ws": "^0.9.16",
    "toast-me": "^1.3.0"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "babel-eslint": "^10.0.3",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.18.3",
    "husky": "^4.2.1",
    "prettier": "^1.19.1"
  },
  "nodemonConfig": {
    "ignore": [
      "src/*"
    ]
  },
  "husky": {
    "hooks": {
      "pre-push": "yarn lint .",
      "pre-commit": "yarn prettier ."
    }
  }
}
